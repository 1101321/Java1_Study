		System.out.println();//改行
		String str1 = "Japanias";
		String str2 = "Japanias";
		if (str1 == str2) {
		System.out.println("同じ");
		} else {
		System.out.println("違う");
		}
		String str3 = new String("Japanias");
		if (str1 == str3) {
		System.out.println("同じ");
		} else {
		System.out.println("違う");
		}
		
		//【問題6-6】
		//下記プログラム追加実行し挙動を確認しなさい。
		System.out.println();//改行
		if (str1.equals(str3)) {
			System.out.println("同じ");
			} else {
			System.out.println("違う");
